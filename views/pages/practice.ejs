<!DOCTYPE html>
<html lang="en">

    <body>
        <h1><%= title %></h1>

        <% mascots.forEach( function(mascot){%>
            <li>
                name: <%= mascot.name %>,
                rep : <%= mascot.organization %>,
                born: <%= mascot.birth_year %>
            </li>
        <% }); %>

    </body>
</html>